						<div class="container_12">
							<div th:replace="almacenamientoPTAP/almacenamientoSecondTable :: resultsListSecond" class="grid_10 height800" id="idSecondTable"></div>
						</div>	
					</div>	
				</fieldset>
				
				
				<div class="espaciado-superior6  col-md-12 col-sm-12 col-xs-12" >				
					<div class="col-md-11 col-sm-11 col-xs-11">
						<fieldset th:fragment="buttons">
							<p class="main_heading">
								<legend th:text="${#messages.msg('muestepaf21.titulo')}"></legend>
							</p>
						</fieldset>
					</div>
					
					<div class="col-md-1 col-sm-1 col-xs-1">
						<fieldset th:fragment="buttons">
							<div class="col-md-12 col-sm-12 col-xs-12 no-padding text-right">
								<button type="button" class="btn btn-info btn-crear-mant-correo" data-toggle="modal" onclick="modificarTableThree(-1)"><span class="glyphicon glyphicon-plus"></span><label th:text="${#messages.msg('tipoProceso.agregar')}"></label></button>					
							</div>
						</fieldset>
					</div>				
				</div>

				<fieldset class="espaciado-superior col-md-12 col-sm-12 col-xs-12" th:fragment="buttons">
					<div id="idTableThreeTable">							
						<div class="container_12">
							<div th:replace="almacenamientoPTAP/almacenamientoThreeTable :: resultsListThree" class="grid_10 height800" id="idThreeTable"></div>
						</div>	

					</div>	

