package pe.com.sedapal.scr.correo.core.beans;

public class AdjuntoMensaje {
	private String strNombreAdjunto;
	private byte[] bytArchivoAdjunto;
	
	public AdjuntoMensaje() {}

	public String getStrNombreAdjunto() {
		return strNombreAdjunto;
	}

	public void setStrNombreAdjunto(String strNombreAdjunto) {
		this.strNombreAdjunto = strNombreAdjunto;
	}

	public byte[] getBytArchivoAdjunto() {
		return bytArchivoAdjunto;
	}

	public void setBytArchivoAdjunto(byte[] bytArchivoAdjunto) {
		this.bytArchivoAdjunto = bytArchivoAdjunto;
	}
}
