package pe.com.sedapal.scr.correo.core.beans;

public class ConfiguracionCorreo {
	private MensajeCorreo mensajeCorreo;
	private CuentaCorreo cuentaCorreo;
	private ServidorArchivos servidorArchivos;
	private ServidorCorreos servidorCorreos;
	
	public ConfiguracionCorreo() {}

	public MensajeCorreo getMensajeCorreo() {
		return mensajeCorreo;
	}

	public void setMensajeCorreo(MensajeCorreo mensajeCorreo) {
		this.mensajeCorreo = mensajeCorreo;
	}

	public CuentaCorreo getCuentaCorreo() {
		return cuentaCorreo;
	}

	public void setCuentaCorreo(CuentaCorreo cuentaCorreo) {
		this.cuentaCorreo = cuentaCorreo;
	}

	public ServidorArchivos getServidorArchivos() {
		return servidorArchivos;
	}

	public void setServidorArchivos(ServidorArchivos servidorArchivos) {
		this.servidorArchivos = servidorArchivos;
	}

	public ServidorCorreos getServidorCorreos() {
		return servidorCorreos;
	}

	public void setServidorCorreos(ServidorCorreos servidorCorreos) {
		this.servidorCorreos = servidorCorreos;
	}
}
