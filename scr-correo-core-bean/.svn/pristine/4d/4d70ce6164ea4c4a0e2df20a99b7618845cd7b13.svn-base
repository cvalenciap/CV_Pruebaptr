package pe.com.sedapal.scr.correo.core.beans;

import java.io.Serializable;
import java.math.BigDecimal;

public class ReporteGraficoBean implements Serializable{
	
	private static final long serialVersionUID = 1L;
	
	private String PERIODO;
	private String NOMRIO;
	private String DIA;	
	private BigDecimal PROMED;
	
	public String getPERIODO() {
		return PERIODO;
	}
	public void setPERIODO(String pERIODO) {
		PERIODO = pERIODO;
	}
	public String getNOMRIO() {
		return NOMRIO;
	}
	public void setNOMRIO(String nOMRIO) {
		NOMRIO = nOMRIO;
	}
	public String getDIA() {
		return DIA;
	}
	public void setDIA(String dIA) {
		DIA = dIA;
	}
	public BigDecimal getPROMED() {
		return PROMED;
	}
	public void setPROMED(BigDecimal pROMED) {
		PROMED = pROMED;
	}	

}
